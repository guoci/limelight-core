<div class="div-table-container selector_data_table_container ">

<div class="div-table-header-row div-table-row">

    {{#each tableObject.columns as |column| }}

      {{#if column.hideColumnHeader}}

        <div style="white-space:nowrap;width:{{column.width}};max-width:{{column.width}};" class="div-table-header-cell column-{{column.id}} blank-header non-sortable-header" data-columnid="{{column.id}}">

            <div style="display:inline-block;width:80%;white-space:normal;">
                &nbsp;
            </div>

        </div>

      {{else}}

        <div style="white-space:nowrap;width:{{column.width}};max-width:{{column.width}};" class="div-table-header-cell column-{{column.id}}  {{#if column.sort}}sortable-header clickable{{else}}non-sortable-header{{/if}}" {{#if column.sort}}data-sortType="{{column.sort}}"{{/if}} data-columnid="{{column.id}}">

            <div style="display:inline-block;width:80%;white-space:normal;{{#if column.style_override}}{{column.style_override}}{{/if}}">
                {{column.displayName}}
            </div>

            <div style="display:inline-block;width:20%;white-space:nowrap;text-align:right;padding-right:3px;">

                {{#if column.sort}}
                    <div style="display:inline-block;{{#if column.lastItem}}padding-right:3px;{{/if}}">
                        <img src="static/images/icon-sort.png" style="max-height:12px;">
                    </div>
                {{/if}}

                {{#if column.lastItem}}

                {{else}}
                    <div style="display:inline-block;padding-right:3px">
                        <svg preserveAspectRatio="none" height="16px" width="2px"><line x1="0" y1="0" x2="0" y2="16" style="stroke:#d3d3d3;stroke-width:2" /></svg>
                    </div>
                {{/if}}

            </div>
        </div>

      {{/if}}

    {{/each}}


</div>

<div class="table-rows-container selector_table_rows_container" style="margin:0px;padding:0px;">
{{#each tableObject.dataObjects as |dataObject| }}

    <div style="position:relative;" 
    	class="selector_data_table_row div-table-data-row div-table-row expandable-table-row mod-data-row {{#if ../tableObject.expandableRows}}clickable{{/if}}" 
    	data-id="{{dataObject.uniqueId}}">

        {{#if ../tableObject.expandableRows}}
            <div style="text-align:center;width:20px;position:absolute;left:-16px;top:5px;" class="expand-icon"><img src="static/images/pointer-right.png" style="max-width:11px;max-height:11px;"></div>
        {{/if}}

        {{#each ../tableObject.columns as |column| }}
            <div style="width:{{column.width}};max-width:{{column.width}};{{#if column.style_override}}{{column.style_override}}{{/if}}"
                 class="div-table-data-cell div-table-cell column-{{column.id}} {{#if column.css_class}}{{column.css_class}}{{/if}}"
                 data-columnid="{{column.id}}"
                 data-value="{{lookup dataObject column.dataProperty }}"
                 data-row-id="{{dataObject.uniqueId}}">

                {{#if column.showHorizontalGraph}}
                    <svg width="{{column.graphWidth}}px" height="14px" preserveAspectRatio="none">
                        <rect x="0" y="0" width="{{column.graphWidth}}px" height="100%" style="fill-opacity:0.0;fill:rgb(255,255,255);stroke-width:2;stroke:#d3d3d3" />
                        <rect x="0" y="0" width="{{lookup dataObject.graphWidths column.id}}px" height="100%" style="fill-opacity:1.0;fill:#32cd32;stroke-width:1;stroke:#d3d3d3" />
                    </svg>
                    {{lookup dataObject column.dataProperty }}
                {{else}}
                    {{lookup dataObject column.dataProperty }}
                {{/if}}
            </div>
        {{/each}}

    </div> 

    {{/each}}

    </div>
</div>
